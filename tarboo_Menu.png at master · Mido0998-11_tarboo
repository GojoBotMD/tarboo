.details-overlay > summary.btn-primary:focus { outline: 2px solid var(--focus-outlineColor, var(--color-accent-fg)); outline-offset: -2px; box-shadow: inset 0 0 0 3px var(--fgColor-onEmphasis, var(--color-fg-on-emphasis)); }

